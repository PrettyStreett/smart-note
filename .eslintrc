{
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "extends": ["airbnb", "airbnb/hooks", "react-app"],
    "rules": {
      "prettier/prettier": "warn",
      "no-alert": "off",
      "no-unused-vars": "warn",
      "no-console": "off",
      "quotes": "off",
      "jsx-quotes": [
        "off",
        "prefer-double"
      ],
      "comma-dangle": "off",
      "arrow-body-style": "off",
      "react/function-component-definition": "off"
    }
  }